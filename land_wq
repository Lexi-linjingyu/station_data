library(readr)
library(readxl)
library(dplyr)
setwd("C:/project/paper5/observed_wq")
ref_s <- read_excel ("station_ref.xls",col_names = FALSE)
some_s <- read_csv("changed_row.csv")
some_s <- some_s %>% 
  select(-c(3,4,5,6,8,13))
colnames(ref_s) <- c("NAME","LAT","LON")
all <- left_join(some_s,ref_s, by = 'NAME')
